{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    [
      "component",
      [
        {
          "libraryName": "element-ui"
        }
      ]
    ],
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node",
        "transform-object-rest-spread"
      ]
    }
  }
}

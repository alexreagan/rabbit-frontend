{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "@babel/preset-stage-0"
  ],
  "plugins": [
    "transform-runtime",
    [
      "component",
      [
        {
          "libraryName": "element-ui"
        }
      ]
    ]
  ],
  "env": {
    "test": {
      "presets": ["env", "@babel/preset-stage-0"],
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
